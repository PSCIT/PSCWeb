
@{
    ViewBag.Title = "Media7Ar";
    Layout = "~/Views/Shared/_LayoutAr.cshtml";
}

<style>
    @@media only screen and (min-width : 320px) {
        .slickclass {
            display: none;
        }

        header {
            background: none;
            height: auto;
        }

        .overlay {
            display: none;
        }

        .footerskew {
            display: none;
        }

        footer {
            margin: 0;
        }

            footer img {
                padding-top: 30px;
            }
    }
</style>

<div style="position:absolute;left:120px;top:10px;z-index:999999999;">
    <a target="_blank" href="https://mail.psc.com.kw/owa" class="mont" style="color:#444">@@</a> |  <a class="mont" style="color:#555" href=@Url.Action("Media7", "Home")>English</a> | <a style="font-family: 'Droid Arabic Kufi', sans-serif;color:#555" href="#">عربي</a>
</div>

<div class="container" style="direction:rtl">

    <div class="row">
        <div class="col-lg-12 mont">
            <br />
            <h2 class="text-center">شركة الخدمات العامة تقيم غبقتها السنوية </h2>
            <br />
            <h4 style="line-height:35px;">

                اقامت شركة الخدمات العامة غبقتها السنوية بحضور لفيف من المسئولين و الموظفين في ميادين الرماية التابعه للشركة الوطنية للرماية بتاريخ 4 يونيو2018


            </h4>
            <br />
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">
            @for (var i = 2; i < 10; i++)
            {
                string path = "/Content/Images/News_Events/Gabka18/NOU-00" + i + ".jpg";
                <div class="gallery">
                    <div class="thumbnail-1 wow fadeInLeft">
                        <img src="@path" />
                    </div>
                    <div class="large-1 wow bounceInLeft">
                        <img src="@path" />
                        <span class="close">&#x2715;</span>
                    </div>
                </div>
            }

            @for (var i = 10; i < 100; i++)
            {
                string path = "/Content/Images/News_Events/Gabka18/NOU-0" + i + ".jpg";
                <div class="gallery">
                    <div class="thumbnail-1 wow fadeInLeft">
                        <img src="@path" />
                    </div>
                    <div class="large-1 wow bounceInLeft">
                        <img src="@path" />
                        <span class="close">&#x2715;</span>
                    </div>
                </div>
            }

            @for (var i = 100; i < 120; i++)
            {
                string path = "/Content/Images/News_Events/Gabka18/NOU-" + i + ".jpg";
                <div class="gallery">
                    <div class="thumbnail-1 wow fadeInLeft">
                        <img src="@path" />
                    </div>
                    <div class="large-1 wow bounceInLeft">
                        <img src="@path" />
                        <span class="close">&#x2715;</span>
                    </div>
                </div>
            }

        </div>
    </div>
</div>

<br /><br /><br />
